	"paths": {
		"/animedb": {
			"get": {
				"operationId": "GetAll",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/AnimeList"
								}
							}
						}
					}
				},
				"security": [],
				"parameters": [
					{
						"in": "query",
						"name": "limit",
						"required": false,
						"schema": {
							"format": "double",
							"type": "number"
						}
					}
				]
			}
		},
		"/animedb/anime/{animeId}": {
			"get": {
				"operationId": "GetOne",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/AnimeItem"
								}
							}
						}
					}
				},
				"security": [],
				"parameters": [
					{
						"in": "path",
						"name": "animeId",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		},
		"/animedb/tags": {
			"get": {
				"operationId": "GetAnimeTags",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"items": {
										"type": "string"
									},
									"type": "array"
								}
							}
						}
					}
				},
				"security": [],
				"parameters": []
			}
		},
		"/auth/signup": {
			"post": {
				"operationId": "Signup",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"anyOf": [
										{
											"$ref": "#/components/schemas/UserSchema"
										},
										{
											"$ref": "#/components/schemas/ErrorSchema"
										}
									]
								},
								"examples": {
									"Example 1": {
										"value": {
											"email": "test@gmail.com",
											"password": "pas34Wo"
										}
									}
								}
							}
						}
					}
				},
				"description": "User registration by email and password",
				"security": [],
				"parameters": [
					{
						"in": "query",
						"name": "email",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"in": "query",
						"name": "password",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		},
		"/auth": {
			"post": {
				"operationId": "Signin",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"anyOf": [
										{
											"properties": {
												"token": {
													"type": "string"
												},
												"user": {
													"$ref": "#/components/schemas/UserSchema"
												}
											},
											"required": [
												"token",
												"user"
											],
											"type": "object"
										},
										{
											"$ref": "#/components/schemas/ErrorSchema"
										}
									]
								},
								"examples": {
									"Example 1": {
										"value": {
											"email": "test@gmail.com",
											"password": "pas34Wo"
										}
									}
								}
							}
						}
					}
				},
				"description": "Authorization of the user by email and password",
				"security": [],
				"parameters": [
					{
						"in": "query",
						"name": "email",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"in": "query",
						"name": "password",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		},
		"/auth/me": {
			"post": {
				"operationId": "Me",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"anyOf": [
										{
											"$ref": "#/components/schemas/UserSchema"
										},
										{
											"$ref": "#/components/schemas/ErrorSchema"
										}
									]
								}
							}
						}
					}
				},
				"security": [
					{
						"api_key": []
					}
				],
				"parameters": [
					{
						"in": "header",
						"name": "Authorization",
						"required": false,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		},
		"/user/update": {
			"put": {
				"operationId": "UserUpdate",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/UserSchema"
								},
								"examples": {
									"Example 1": {
										"value": {
											"token": "token",
											"displayName": "name",
											"photoURL": "https://some.jpg"
										}
									}
								}
							}
						}
					}
				},
				"description": "User update fields",
				"security": [],
				"parameters": [
					{
						"in": "query",
						"name": "token",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"in": "query",
						"name": "displayName",
						"required": false,
						"schema": {
							"type": "string"
						}
					},
					{
						"in": "query",
						"name": "photoURL",
						"required": false,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		}
