		"schemas": {
			"AnimeTypes": {
				"type": "string",
				"enum": [
					"Special",
					"Movie",
					"OVA",
					"ONA",
					"TV",
					""
				]
			},
			"AnimeStatuses": {
				"type": "string",
				"enum": [
					"FINISHED",
					"UPCOMING",
					"UNKNOWN",
					"CURRENTLY",
					""
				]
			},
			"AnimeSeason": {
				"type": "string",
				"enum": [
					"SUMMER",
					"UNDEFINED",
					"WINTER",
					"SPRING",
					"FALL",
					""
				]
			},
			"AnimeTags": {
				"type": "string",
				"enum": [
					"alternative present",
					"amnesia",
					"anti-hero",
					"asia",
					"based on a manga",
					"contemporary fantasy",
					"cops",
					"crime",
					"criminals",
					"demons",
					"detective",
					"detectives",
					"drama",
					"earth",
					"espionage",
					"gods",
					"japan",
					"male protagonist",
					"manga",
					"mind games",
					"mystery",
					"overpowered main characters",
					"philosophy",
					"plot continuity",
					"police",
					"present",
					"primarily adult cast",
					"primarily male cast",
					"psychological",
					"psychopaths",
					"revenge",
					"rivalries",
					"secret identity",
					"serial killers",
					"shinigami",
					"shounen",
					"supernatural",
					"thriller",
					"time skip",
					"tragedy",
					"urban",
					"urban fantasy",
					"vigilantes",
					"work"
				]
			},
			"AnimeItem": {
				"properties": {
					"_id": {
						"type": "string"
					},
					"sources": {
						"items": {
							"type": "string"
						},
						"type": "array"
					},
					"title": {
						"type": "string"
					},
					"type": {
						"$ref": "#/components/schemas/AnimeTypes"
					},
					"episodes": {
						"type": "number",
						"format": "double"
					},
					"status": {
						"$ref": "#/components/schemas/AnimeStatuses"
					},
					"animeSeason": {
						"properties": {
							"year": {
								"type": "number",
								"format": "double"
							},
							"season": {
								"$ref": "#/components/schemas/AnimeSeason"
							}
						},
						"required": [
							"year",
							"season"
						],
						"type": "object"
					},
					"picture": {
						"type": "string"
					},
					"thumbnail": {
						"type": "string"
					},
					"synonyms": {
						"items": {
							"type": "string"
						},
						"type": "array"
					},
					"relations": {
						"items": {
							"type": "string"
						},
						"type": "array"
					},
					"tags": {
						"items": {
							"$ref": "#/components/schemas/AnimeTags"
						},
						"type": "array"
					}
				},
				"required": [
					"_id",
					"sources",
					"title",
					"type",
					"episodes",
					"status",
					"animeSeason",
					"picture",
					"thumbnail",
					"synonyms",
					"relations",
					"tags"
				],
				"type": "object",
				"additionalProperties": false
			},
			"AnimeList": {
				"items": {
					"$ref": "#/components/schemas/AnimeItem"
				},
				"type": "array"
			},
			"UserSchema": {
				"properties": {
					"uid": {
						"type": "string"
					},
					"displayName": {
						"type": "string"
					},
					"photoURL": {
						"type": "string"
					},
					"email": {
						"type": "string"
					},
					"emailVerified": {
						"type": "boolean"
					},
					"phoneNumber": {
						"type": "string",
						"nullable": true
					},
					"isAnonymous": {
						"type": "boolean"
					},
					"tenantId": {
						"type": "string",
						"nullable": true
					}
				},
				"required": [
					"uid",
					"displayName",
					"photoURL",
					"email",
					"emailVerified",
					"phoneNumber",
					"isAnonymous",
					"tenantId"
				],
				"type": "object",
				"additionalProperties": false
			},
			"ErrorSchema": {
				"properties": {
					"message": {
						"type": "string"
					},
					"code": {
						"type": "string"
					}
				},
				"required": [
					"message",
					"code"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
